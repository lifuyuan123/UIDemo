<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.administrator.uidemo.FloatImageActivity">

    <com.example.administrator.uidemo.view.FloatImage
        android:id="@+id/image"
        android:layout_centerInParent="true"
        android:src="@mipmap/ic_launcher"
        android:layout_width="50dp"
        android:layout_height="50dp" />


    <com.example.administrator.uidemo.view.DragFloatActionButton
        android:id="@+id/fab"
        android:src="@mipmap/ic_launcher"
        app:backgroundTint="@color/color_green"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />


</RelativeLayout>
